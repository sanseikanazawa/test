package jp.co.internous.action;

public class Person {
	private String name=null;
	private int age=0;


	public Person(String name,int age){
		this.name=name;
		this.age=age;
	}

	private String getName(){
		return this.name;
	}

	private void setName(String name){
		this.name=name;
	}


}









